{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "cwd": "${workspaceRoot}",
      "env": {
        "MYSQL_DB_SERVER": "localhost",
        "MYSQL_DB_PORT": "3306",
        "MYSQL_DB_NAME": "freeradius",
        "MYSQL_DB_USER": "login",
        "MYSQL_DB_PW": "login",
        "GECO_API_URL": "http://example.com:1234",
        "GECO_API_KEY": "blub"
      },
      "args": ["-log-level", "debug", "-log-format", "console"]
    }
  ]
}
